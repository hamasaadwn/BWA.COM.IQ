extends layout

block content
    // Dealer-profile
    section.dealer_profile.inner_pages(style='padding-top: 1%')
        .container
            ul.coustom-breadcrumb
                li
                    a(href='/', style='color: #1e96fe') #{translation.HOME_TXT}
                li
                    a(style='color: #1e96fe') >
                li(style='color: #1e96fe') #{translation.DEALER_PROFILE}
            .row
                .col-md-3.col-sm-3.col-xs-4
                    .dealer_logo
                        img(src='/uploads/avatars/'+pro.avatar, alt='image')
                .col-md-6.col-sm-5.col-xs-8
                    .dealer_info
                        h4 #{pro.company}
                        if pro.address.city == "Hawler"
                            p #{translation.CITY_HAWLER} - #{pro.address.street}
                        if pro.address.city == "Slemany"
                            p #{translation.CITY_SLEMANY}- #{pro.address.street}
                        if pro.address.city == "Duhok"
                            p #{translation.CITY_DUHOK}- #{pro.address.street}
                        if pro.address.city == "Halabja"
                            p #{translation.CITY_HALABJA}- #{pro.address.street}
                        if pro.address.city == "Kirkuk"
                            p #{translation.CITY_KIRKUK}- #{pro.address.street}
                        if pro.address.city == "Baghdad"
                            p #{translation.CITY_BAGHDAD}- #{pro.address.street}
                        if pro.address.city == "Mosul"
