                        if pro.address.city == "Diyala"
                            p #{translation.CITY_DIYALA}- #{pro.address.street}
                        if pro.address.city == "Wasit"
                            p #{translation.CITY_WASIT}- #{pro.address.street}
                        if pro.address.city == "Maysan"
                            p #{translation.CITY_MAYSAN}- #{pro.address.street}
                        if pro.address.city == "Dhi Qar"
                            p #{translation.CITY_DHI_QAR}- #{pro.address.street}
                        if pro.address.city == "Al Muthana"
                            p #{translation.CITY_AL_MUTHANA}- #{pro.address.street}
                        if pro.address.city == "Al-Qadisiyah"
                            p #{translation.CITY_AL_QADISIYAH}- #{pro.address.street}
                        if pro.address.city == "Babil"
                            p #{translation.CITY_BABIL}- #{pro.address.street}
                        if pro.address.city == "Karbala"
                            p #{translation.CITY_KARBALA}- #{pro.address.street}
                        if pro.address.city == "Najaf"
                            p #{translation.CITY_NAJAF}- #{pro.address.street}
                        if pro.address.city == "Al Anbar"
                            p #{translation.CITY_AL_ANBAR}- #{pro.address.street}
                        ul.dealer_social_links
                            if pro.socialLinks.facebook
                                li.facebook-icon
                                    a(href=pro.socialLinks.facebook)
                                        i.fa.fa-facebook-square(aria-hidden='true')
                            if pro.socialLinks.instagram
                                li.instagram-icon
                                    a(href=pro.socialLinks.instagram)
                                        i.fa.fa-instagram(aria-hidden='true')
                            if pro.socialLinks.youtube
                                li.youtube-icon
                                    a(href=pro.socialLinks.youtube)
                                        i.fa.fa-youtube(aria-hidden='true')
                            if pro.socialLinks.twitter
                                li.twitter-icon
                                    a(href=pro.socialLinks.twitter)
                                        i.fa.fa-twitter(aria-hidden='true')
                .col-md-3.col-sm-4.col-xs-12
                    if pro.website
                        .dealer_contact_info.gray-bg
                            h6
                                i.fa.fa-globe(aria-hidden='true')
                                |  #{translation.WEBSITE}
                            a(href=pro.website)=pro.website
                    if pro.phoneNumber
                    .dealer_contact_info.gray-bg
                        h6
                            i.fa.fa-phone(aria-hidden='true')
                            |  #{translation.PHONE_NUMBER}
                        a(href='tel:'+pro.phoneNumber)=pro.phoneNumber
