            .row
                .col-md-9
                    .dealer_more_info
                        h5.gray-bg.info_title  #{translation.ABOUT} #{pro.company}
                        p=pro.info
                        .dealer_listings
                            h6 #{translation.AGENCY_LISTING_CARS}
